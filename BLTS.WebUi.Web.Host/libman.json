{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.js",
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      "library": "font-awesome@5.15.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "bootstrap@4.5.3",
      "provider": "jsdelivr",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "dist/js/bootstrap.bundle.js",
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.js.map",
        "dist/js/bootstrap.bundle.min.js.map",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.min.css",
        "dist/css/bootstrap.min.css.map",
        "dist/css/bootstrap-grid.css",
        "dist/css/bootstrap-grid.min.css",
        "dist/css/bootstrap-grid.min.css.map",
        "dist/css/bootstrap-reboot.css",
        "dist/css/bootstrap-reboot.min.css",
        "dist/css/bootstrap-reboot.min.css.map"
      ]
    },
    {
      "library": "admin-lte@3.0.5",
      "provider": "jsdelivr",
      "destination": "wwwroot/lib/admin-lte",
      "files": [
        "dist/js/adminlte.js",
        "dist/js/adminlte.js.map",
        "dist/js/adminlte.min.js",
        "dist/js/adminlte.min.js.map",
        "dist/js/demo.js",
        "dist/js/demo.min.js",
        "dist/css/adminlte.css",
        "dist/css/adminlte.css.map",
        "dist/css/adminlte.min.css",
        "dist/css/adminlte.min.css.map",
        "dist/img/AdminLTELogo.png",
        "dist/img/avatar.png",
        "dist/img/avatar2.png",
        "dist/img/avatar3.png",
        "dist/img/avatar04.png",
        "dist/img/avatar5.png",
        "dist/img/boxed-bg.jpg",
        "dist/img/boxed-bg.png",
        "dist/img/icons.png"
      ]
    }

  ]
}