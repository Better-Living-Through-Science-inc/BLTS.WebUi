@using BLTS.WebUi.Web.Startup
@{ ViewBag.CurrentPageName = "SoftwareEngineering";
  ViewBag.Title = "Software Engineering"; }
<div class="content-header">
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-12">
        <h1 class="m-0 text-dark">@ViewBag.Title</h1>
      </div>
    </div>
  </div>
</div>
<div class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-header border-transparent">
            <h2 class="card-title">Artificial Intelegence</h2>
            <div class="card-tools">
              <button type="button" class="btn btn-tool" data-card-widget="collapse">
                <i class="fas fa-minus"></i>
              </button>
            </div>
          </div>
          <div class="card-body">
            <h4>Automated process improvement</h4>
            <p>
              Use AI machine learning to autonomously monitor and analyze all processes<br />
              Use AI results to speed assembly, tune equipment and reduce waste<br />
            </p>
            <h4>Polymorphic AI</h4>
            <p>
              Use the AI to recursively generate software code with self-improvement algorithms
            </p>
            <h4>Automated manufacturing</h4>
            <p>
              Software controlled multiple source IBID and particle accelerator
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
